<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="Wisata Semarang"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WisataSemarang"
        tools:targetApi="31">
        <activity
            android:name=".SplashActivity"
            android:theme="@style/Theme.WisataSemarang.Splash"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".WisataData"
            android:exported="false"
            android:label="@string/title_activity_wisata_data"
            android:theme="@style/Theme.WisataSemarang" />
        <activity
            android:name=".DetailWisataActivity"
            android:exported="false"
            android:label="@string/title_activity_detail_wisata"
            android:theme="@style/Theme.WisataSemarang" />
        <activity
            android:name=".ListWisataActivity"
            android:exported="false"
            android:label="@string/title_activity_list_wisata"
            android:theme="@style/Theme.WisataSemarang" />
        <activity
            android:name=".HomeActivity"
            android:exported="false"
            android:label="@string/title_activity_home"
            android:theme="@style/Theme.WisataSemarang" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.WisataSemarang">
        </activity>
    </application>

</manifest>